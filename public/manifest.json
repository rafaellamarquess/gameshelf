{
  "name": "GameShelf",
  "short_name": "GameShelf",
  "description": "Sua Biblioteca de Jogos",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#1f2937",
  "scope": "/",
  "icons": [
    {
      "src": "/icons/android/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/icons/android/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "prefer_related_applications": false,
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.example.gameshelf",
      "id": "com.example.gameshelf"
    }
  ],
  "orientation": "portrait",
  "launch_handler": {
    "action": "open_app",
    "client_mode": "auto"
  },
  "protocol_handlers": [
    {
      "protocol": "web+gameshelf",
      "url": "https://game-shelf.netlify.app/handle/{action}"
    }
  ],
"share_target": {
  "action": "/share",
  "method": "POST",
  "enctype": "multipart/form-data",
  "params": {
    "title": "name",
    "text": "description",
    "url": "url"
  }
},
  "background_sync": {
    "name": "syncData",
    "min_interval": 24
  },
  "offline_capability": true,
  "scope_extensions": ["/"],
  "categories": ["games", "entertainment"],
  "dir": "ltr",
  "iarc_rating_id": "com.example.gameshelf.rating",
  "lang": "pt-BR",
  "handle_links": true,
  "widgets": [
    {
      "widget_type": "app_launch",
      "url": "/launch_widget",
      "title": "Launch GameShelf"
    }
  ]
}
